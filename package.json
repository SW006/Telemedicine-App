{
  "name": "tele-tabib-integration",
  "version": "1.0.0",
  "description": "TeleTabib Medical Booking Application - Full Stack Integration",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd front-end && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "install:all": "npm install && cd front-end && npm install && cd ../backend && npm install",
    "build": "cd front-end && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd front-end && npm run start",
    "start:backend": "cd backend && npm run start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "front-end",
    "backend"
  ],
  "dependencies": {
    "axios": "^1.12.1"
  }
}
